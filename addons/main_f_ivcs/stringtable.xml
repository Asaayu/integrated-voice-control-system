            </Key>
            <Key ID="STR_IVCS_PAUSE_MENU_SPEECH_BUTTON_NAME">
                <Original>Windows Speech Settings</Original>
            </Key>
            <Key ID="STR_IVCS_PAUSE_MENU_SPEECH_BUTTON_TOOLTIP">
