	<!-- Specific phrases -->
	<!-- All possible digit grid position selections -->
	<rule id="grid_position" scope="public">
		<!-- Can select 6-8 digits for each grid position -->
		<item repeat="6-8"><ruleref uri="#digits"/></item>
	</rule>

	<!-- All possible selections -->
	<rule id="selection" scope="public">
		<!-- Can select as many units in a single call as needed -->
		<one-of>
			<item></item>
			<item><ruleref uri="#unit_selection"/></item>
			<item><ruleref uri="#group_selection"/></item>
		</one-of>
	</rule>
	<rule id="unit_selection" scope="public">
		<!-- Can select as many units in a single call as needed -->
		<item repeat="1-"><ruleref uri="#unit_number"/></item>
	</rule>
	<rule id="group_selection" scope="public">
		<!-- Can select as many groups in a single call as needed -->
		<item repeat="1-"><ruleref uri="#group_name"/></item>
	</rule>

	<!-- Select a type of target -->
	<rule id="object_selection" scope="public">
